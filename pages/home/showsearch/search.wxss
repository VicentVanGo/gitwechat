.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td1 {
    width:30%;
    justify-content: center;
    text-align: center;
}
.td2 {
    width:70%;
    justify-content: center;
    text-align: center;
    font-size: small;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th1 {
  width: 30%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}
.th2 {
  width: 70%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}